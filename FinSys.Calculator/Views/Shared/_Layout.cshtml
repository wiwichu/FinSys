            <li><a href="#/CashFlows" >Cash Flows</a></li>
            <li><a href="#/USTBill" >USTBill</a></li>
            @*<li class="active"><a asp-controller="App" asp-action="Index">Home</a></li>
                <li><a asp-controller="App" asp-action="About">About</a></li>
                <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
                <li><a asp-controller="App" asp-action="CustomCalc">Custom Calcs</a></li>*@
        </ul>
    </div>
    <div id="wrapper">
        <nav nav class="navbar navbar-inverse">
            <div id="main" class="container-fluid">
                <ul class="nav navbar-nav navbar-left">
                    <li>
                        <button id="sidebarToggle" class="btn btn-primary">
                            <i class="fa fa-angle-left"></i>
                        </button>

                    </li>
                </ul>
                <a asp-controller="App" asp-action="Index" class="navbar-brand">FinSys</a>
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="#" class="btn btn-sm btn-info">
                            <i class="fa fa-save"></i> Save
